{
  "name": "llm-meeting",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:firebase": "tsc -b && vite build --mode production",
    "build:single": "tsc -b && vite build --config vite.config.single.ts",
    "lint": "eslint .",
